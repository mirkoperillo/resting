{
  "description": "rest client focused on organization of the saved requests",
  "manifest_version": 2,
  "name": "Resting",
  "version": "1.2.1",
  "author" : "Mirko Perillo",
  "homepage_url": "https://github.com/mirkoperillo/resting",
  "icons": {
    "32": "icons/letter-r_32.png",
    "64": "icons/letter-r_64.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/letter-r_32.png"
  },
  "content_security_policy" : "default-src * 'unsafe-inline'; script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "downloads"
  ]
}
